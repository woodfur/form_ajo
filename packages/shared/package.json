{
  "name": "formfn-shared",
  "version": "1.0.0",
  "private": true,
  "description": "Shared module for sharing code within formfn-monorepo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "prettier --check 'src/**/*.ts'",
    "eslint": "eslint --cache 'src/**/*.ts'",
    "lint": "yarn prettier --write && yarn eslint --fix",
    "tsc": "rm -r dist/*; tsc -P tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests"
  },
  "dependencies": {
    "auction-house": "https://github.com/AJO-Ltd/auction-house.git",
    "candy-machine": "https://github.com/AJO-Ltd/candy-machine.git",
    "gumdrop": "https://github.com/AJO-Ltd/gumdrop.git",
    "@metaplex-foundation/mpl-token-metadata": "2.7.0",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "1.73.0",
    "add": "^2.0.6",
    "bad-words": "^3.0.4",
    "bignumber.js": "^9.1.0",
    "dayjs": "^1.10.7",
    "email-validator": "^2.0.4",
    "tiny-invariant": "^1.2.0",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/bad-words": "^3.0.1",
    "@types/email-validator": "^1.0.6",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jest": "^27.4.5",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0"
  }
}
